{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Express Auth with React/Redux",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "prettier-eslint \"**/*.js\" --write"
  },
  "author": "Praveen Bisht",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.3",
    "nodemon": "^1.14.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  }
